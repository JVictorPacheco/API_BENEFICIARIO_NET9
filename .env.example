# Ambiente da aplicação
ASPNETCORE_ENVIRONMENT=Production

# Configurações do Banco de Dados
POSTGRES_DB=beneficiario_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here

# Connection String (Production)
DATABASE_CONNECTION_STRING=Host=postgres;Port=5432;Database=beneficiario_db;Username=postgres;Password=your_secure_password_here

# pgAdmin (opcional para produção)
PGADMIN_DEFAULT_EMAIL=admin@admin.com
PGADMIN_DEFAULT_PASSWORD=your_pgadmin_password_here
